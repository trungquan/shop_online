<% provide(:title, "Edit user") %> 
<h1>Update profile</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@user) do |ff| %>
      <%= render 'shared/error_messages', object: ff.object %>
      <%= render 'fields', f: ff %>
      <%= ff.submit "Save changes", class: "btn btn-large btn-success" %>
    <% end %>
  </div>
</div>